{
    "manifest_version": 1,
    "name": "任务栏歌词",
    "slug": "Taskbar-Lyrics",
    "description": "在任务栏上嵌入歌词，目前仅建议Windows 11，且任务栏未被第三方软件修改",
    "preview": "preview.png",
    "version": "1.6.2",
    "author": "沫烬染",
    "author_links": [
        "https://github.com/mo-jinran"
    ],
    "type": "extension",
    "loadAfter": [
        "liblyric",
        "RefinedNowPlaying"
    ],
    "requirements": [
        "liblyric"
    ],
    "injects": {
        "Main": [
            {
                "file": "base.js"
            },
            {
                "file": "lyric.js"
            },
            {
                "file": "func.js"
            },
            {
                "file": "view.js"
            }
        ]
    }
}